{
  "env": {
    "browser": true,
    "es6": false,
    "jquery": true
  },
  "parserOptions": {
    "sourceType": "script"
  },
  "plugins": [
    "html"
  ],
  "settings": {
    "html/indent": "+2"
  },
  "extends": "airbnb",
  "rules": {
    "no-var": 0,
    "func-names": 0,
    "prefer-arrow-callback": 0,
    "space-before-function-paren": [2, "never"],
    "strict": [2, "global"]
  }
}
